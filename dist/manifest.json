{
  "name": "Todo",
  "version": "1.0",
  "description": "Todo list",
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "activeTab",
    "declarativeContent",
    "unlimitedStorage",
    "storage",
    "http://*/*",
    "https://*/*",
    "*://*/*"
  ],
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "logo192.png",
      "32": "logo192.png",
      "48": "logo192.png",
      "128": "logo192.png"
    },
    "default_title": "Todo"
  },
  "icons": {
    "16": "logo192.png",
    "32": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  }
}
